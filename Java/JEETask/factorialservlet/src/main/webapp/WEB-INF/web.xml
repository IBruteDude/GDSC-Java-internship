<web-app>
  <display-name>Factorial Table WebApp</display-name>
  <servlet>
		<servlet-name>java-intern</servlet-name>
		<servlet-class>FactorialServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>java-intern</servlet-name>
		<url-pattern>/factorial-table</url-pattern>
	</servlet-mapping>
</web-app>
